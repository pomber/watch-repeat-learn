{
    "firebase": "watchrepeatlearn",
    "public": "build/bundled",
    "ignore": [
        "README.md",
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
    ],
    "rewrites": [
        {
            "source": "**/!{*.*}",
            "destination": "/index.html"
        }
    ]
}